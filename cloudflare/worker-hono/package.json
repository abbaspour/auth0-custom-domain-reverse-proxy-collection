{
  "name": "auth0-clouflare-hono-custom-domain-reverse-proxy",
  "version": "0.1.0",
  "description": "An Auth0 self-managed custom domain reverse proxy hosted on Cloudflare worker with Hono",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "compile": "tsc",
    "build": "esbuild src/index.ts --bundle --minify --format=esm --outfile=dist/index.js",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write ."
  },
  "dependencies": {
    "hono": "^4.8.5"
  },
  "repository": {
    "url": "https://github.com/abbaspour/auth0-custom-domain-reverse-proxy-collection"
  },
  "author": {
    "name": "Amin Abbaspour",
    "email": "support@auth0.com"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250722.0",
    "@eslint/js": "^9.31.0",
    "esbuild": "^0.20.2",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.8",
    "prettier": "^3.6.2",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.38.0",
    "wrangler": "^4.25.0"
  }
}
